{
  "name": "generator-w3-generator",
  "version": "0.0.42",
  "description": "Gerador inicial de apis usando a arquitetura definida pelo time de backend de .net",
  "homepage": "w3as.com.br",
  "author": {
    "name": "Iago Cavalcante",
    "email": "iagoangelimc@gmail.com",
    "url": "iagocavalcante.com.br"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    ".netcore",
    "api",
    "ddd",
    "nodejs",
    "vuejs",
    "spring",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "3.0.4",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.10.0",
    "eslint-config-xo": "0.26.0",
    "eslint-plugin-prettier": "2.7.0",
    "husky": "0.14.3",
    "jest": "22.4.4",
    "lint-staged": "8.2.1",
    "nsp": "3.2.1",
    "prettier": "1.17.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "findup-sync": "^0.4.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^0.6.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "nconf": "^0.10.0",
    "uuid": "^2.0.1",
    "vs_projectname": "^1.0.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "dotnet": {
    "lts": {
      "version": "1.0.4",
      "targetFramework": "netcoreapp1.0"
    },
    "current": {
      "version": "2.0.2",
      "targetFramework": "netcoreapp2.0"
    }
  },
  "repository": "iagocavalcante/w3-generator",
  "license": "MIT"
}
